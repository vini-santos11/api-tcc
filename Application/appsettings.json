{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "Connection": "Server=;Database=db_tcc;Uid=;Password="
  },
  "TokenConfiguration": {
    "Audience": "TCC.API",
    "Issuer": "TCC.API",
    "Seconds": 86400,
    "Key": "tcc-authentication-valid"
  },
  "PasswordConfiguration": {
    "RequireDigit": false,
    "RequiredLength": 3,
    "RequireNonAlphanumeric": false,
    "RequireUppercase": false,
    "RequireLowercase": false,
    "RequireConfirmedEmail": false,
    "UserLockoutEnabled": true,
    "LockoutTimeSpan": 5,
    "LockoutMaxFailedAccess": 5
  },
  "AllowedHosts": "*"
}
